{
  "manifest_version": 3,
  "name": "BookUp",
  "version": "0.0.0",
  "description": "Actively backup your bookmarks",
  "permissions": [
    "bookmarks",
    "downloads",
    "storage",
    "alarms"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "action": {
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "extension@bookup"
    }
  }
}
